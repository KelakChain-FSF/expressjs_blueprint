{
  "name": "expressjs_blueprint",
  "version": "0.1.0",
  "description": "Nodejs & Typescript and expressjs Api",
  "packageManager": "yarn@4.6.0",
  "type": "module",
  "scripts": {
    "start": "node dist/index.js",
    "build": "tsc && cpx 'src/swagger.json' dist",
    "dev": "nodemon -watch src ./src/index.ts",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev --name init && prisma generate",
    "prisma:studio": "prisma studio",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write src",
    "swagger:generate": "node --loader ts-node/esm src/config/swagger.ts",
    "dev:swagger": "yarn swagger:generate && touch src/swagger.json",
    "prepare": "husky"
  },
  "dependencies": {
    "@prisma/client": "^6.5.0",
    "apollo-server-express": "^3.13.0",
    "aws-lambda": "^1.0.7",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-graphql": "^0.12.0",
    "graphql": "^16.10.0",
    "pino": "^9.6.0",
    "pino-http": "^10.4.0",
    "redis": "^4.7.0",
    "reflect-metadata": "^0.2.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.22.0",
    "@types/aws-lambda": "^8.10.148",
    "@types/copyfiles": "^2",
    "@types/cors": "^2.8.17",
    "@types/cpx": "^1",
    "@types/express": "^5.0.0",
    "@types/node": "^22.13.10",
    "@types/supertest": "^6.0.3",
    "@types/swagger-jsdoc": "^6",
    "@types/swagger-ui-express": "^4.1.8",
    "@typescript-eslint/eslint-plugin": "^8.26.1",
    "@typescript-eslint/parser": "^8.26.1",
    "@vitest/coverage-v8": "3.1.3",
    "@vitest/ui": "3.1.3",
    "copyfiles": "^2.4.1",
    "cpx": "^1.5.0",
    "eslint": "^9.22.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-prettier": "^5.2.3",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.0",
    "nodemon": "^3.1.9",
    "pino-pretty": "^13.0.0",
    "prettier": "^3.5.3",
    "prisma": "^6.5.0",
    "supertest": "^7.1.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.26.1",
    "vite": "^6.3.5",
    "vitest": "^3.1.3"
  },
  "keywords": [
    "nodejs",
    "typescript",
    "expressjs",
    "api"
  ],
  "author": "Majid Ahmaditabar",
  "license": "ISC"
}
